<application xmlns="http://gaellalire.fr/vestige/APPLICATION" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://gaellalire.fr/vestige/APPLICATION http://gaellalire.fr/vestige/application-1.0.0.xsd"
  javaSpecificationVersion="1.6">
  <launcher class="fr.gaellalire.vestige_app.demo3.mod1.Mod1" >
    <mavenResolver mode="FIXED_DEPENDENCIES" groupId="fr.gaellalire.vestige_app.demo3" artifactId="demo3.mod1" version="${project.version}">
       <activateNamedModules />
    </mavenResolver>
    <inject serviceClassName="fr.gaellalire.vestige.spi.resolver.maven.VestigeMavenResolver" setterName="setMavenResolver"/>
  </launcher>
  <configurations>
    <permissions>
      <permission type="java.security.AllPermission" />
    </permissions>
    <mavenConfig>
      <additionalRepository id="gaellalire-repo" url="https://gaellalire.fr/nexus/content/repositories/releases/" />
    </mavenConfig>
  </configurations>
</application>
